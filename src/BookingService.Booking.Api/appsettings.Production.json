{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "/var/logs/booking-service-bookings/log-.txt",
          "rollingInterval": "Day",
          "shared": true
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "BookingService"
    }
  },
  "ConnectionStrings": {
    "BookingsContext": "server=localhost;port=5432;database=bookings;uid=bookings_admin;pwd=admin_bookings"
  },
  "BookingCatalogRestOptions": {
    "BaseAdress": "http://booking-service_catalog-host:8080"
  }
}